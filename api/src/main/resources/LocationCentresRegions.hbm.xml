<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.mdrtbdashboard.model">
	<class name="LocationCentresRegions" table="location_centres_regions" batch-size="500">
		<id name="id" type="int" column="region_id" unsaved-value="0">
				<generator class="native" />
		</id>
		<property name="name" column="name"  />
		<property name="createdOn" column="created_on"  />
		<many-to-one name="creator" class="org.openmrs.Person" column="created_by" cascade="none"  />
	</class>
</hibernate-mapping>
