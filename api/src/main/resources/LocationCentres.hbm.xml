<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.mdrtbdashboard.model">
	<class name="LocationCentres" table="location_centres" batch-size="500">
		<id name="id" type="int" column="centre_id" unsaved-value="0">
				<generator class="native" />
		</id>
		<property name="serialNumber" column="serial_number"  />
		<many-to-one name="location" class="org.openmrs.Location" column="location_id" cascade="none"  />
		<many-to-one name="region" class="org.openmrs.module.mdrtbdashboard.model.LocationCentresRegions" column="region_id" cascade="none" lazy="false" />
		<many-to-one name="agency" class="org.openmrs.module.mdrtbdashboard.model.LocationCentresAgencies" column="agency_id" cascade="none" lazy="false" />
		<property name="createdOn" column="created_on"  />
		<many-to-one name="creator" class="org.openmrs.Person" column="created_by" cascade="none"  />
	</class>
</hibernate-mapping>
