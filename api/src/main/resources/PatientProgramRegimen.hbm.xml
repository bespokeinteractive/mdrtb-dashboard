<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.mdrtbdashboard.model">
	<class name="PatientProgramRegimen" table="patient_program_regimen" batch-size="500">
		<id name="id" type="int" column="id" unsaved-value="0">
            <generator class="native" />
		</id>
		<property name="name" column="name" type="java.lang.String" />
		<property name="startedOn" column="start_date" type="java.util.Date" />
		<property name="finishedOn" column="finish_date" type="java.util.Date" />
		<many-to-one name="programDetails" class="org.openmrs.module.mdrtbdashboard.model.PatientProgramDetails" column="program_details" cascade="none"  />
	</class>
</hibernate-mapping>
