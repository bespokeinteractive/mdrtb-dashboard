<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.mdrtbdashboard.model">
	<class name="PatientProgramDetails" table="patient_program_details" batch-size="500">
		<id name="id" type="int" column="id" unsaved-value="0">
				<generator class="native" />
		</id>
		<property name="tbmuNumber" column="tbmu_number"  />
		<property name="labNumber" column="lab_number"  />
		<property name="details" column="details"  />
		<many-to-one name="patientProgram" class="org.openmrs.PatientProgram" column="patient_program_id" cascade="none"  />
	</class>
</hibernate-mapping>
